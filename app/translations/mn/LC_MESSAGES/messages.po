msgid "Welcome to Your Fuel Tracker!"
msgstr "Шатахуун хянагчдаа тавтай морил!"

#: app/templates/index.html:5
msgid "Track your fuel consumption and monitor your car's efficiency."
msgstr "Шатахууны хэрэглээг хянаж, машины үр ашгийг хянана уу."

#: app/templates/index.html:8
msgid "Quick Stats"
msgstr "Хурдан статистик"

#: app/templates/index.html:12
msgid "Total Fill-ups"
msgstr "Нийт цэнэглэлт"

#: app/templates/index.html:18
msgid "Average Efficiency"
msgstr "Дундаж үр ашиг"

#: app/templates/index.html:25
msgid "Total Spent"
msgstr "Нийт зарцуулсан"

#: app/templates/index.html:32
msgid "Current Fuel Status"
msgstr "Одоогийн шатахууны төлөв"

#: app/templates/index.html:36
msgid "Tank Capacity"
msgstr "Савны багтаамж"

#: app/templates/index.html:40
msgid "Remaining Fuel"
msgstr "Үлдсэн шатахуун"

#: app/templates/index.html:42
msgid "full"
msgstr "бүрэн"

#: app/templates/index.html:45
msgid "Predicted Range"
msgstr "Урьдчилан таамагласан зай"

#: app/templates/index.html:50
msgid "Based on"
msgstr "Суурилсан"

#: app/templates/index.html:50
msgid "efficiency"
msgstr "үр ашиг"

#: app/templates/index.html:58
msgid "Record Your First Fill-up"
msgstr "Анхны цэнэглэлтээ бүртгэх"

#: app/templates/index.html:60
msgid "Add Another Fill-up"
msgstr "Өөр цэнэглэлт нэмэх"

#: app/templates/index.html:65
msgid "Check Driving Range"
msgstr "Жолоодох зайг шалгах"

#: app/templates/index.html:69
